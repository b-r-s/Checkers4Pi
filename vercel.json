{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/validation-key.txt",
      "destination": "/validation-key.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}